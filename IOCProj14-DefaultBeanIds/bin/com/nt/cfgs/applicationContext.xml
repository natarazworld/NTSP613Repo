<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.nt.beans.SampleBean"> <!-- default bean id ::com.nt.beans.SampleBean, com.nt.beans.SampleBean#0 -->
       <property name="name" value="raja"/>
       <property name="age" value="30"/>
    </bean>
    
    <bean class="com.nt.beans.SampleBean"> <!-- default bean id ::com.nt.beans.SampleBean#1 --> 
       <property name="name" value="rajesh"/>
       <property name="age" value="40"/>
    </bean>
    
    <bean class="com.nt.beans.SampleBean"> <!-- default bean id ::com.nt.beans.SampleBean#2  -->
       <property name="name" value="sunil"/>
       <property name="age" value="50"/>
    </bean>
    
    <bean class="java.util.Date"/>  <!-- default bean id :: java.util.Date or  java.util.Date#0  -->
    <bean class="java.util.Date">  <!-- default bean id :: java.util.Date#1 -->
       <property name="date" value="10"/>
    </bean>
    
    

</beans>
